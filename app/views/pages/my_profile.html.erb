<div class="container">
  <div class="row">
    <div class="col-md-12">
    <h2><%= current_user.name %> | <%= current_user.username %></h2>
    </div>
    <div class="col-md-3 profile-pic">
      <%= image_tag current_user.image %>
    </div>
    <div class="col-md-9">
      <h3>Bio</h3>
      This is where the bio will be
    </div>
  </div>
  <hr>

  <% @blogs.each do |b| %>
    <h1><%= link_to b.title, b %></h1>
    <%= b.body %>
  <% end %>

</div>